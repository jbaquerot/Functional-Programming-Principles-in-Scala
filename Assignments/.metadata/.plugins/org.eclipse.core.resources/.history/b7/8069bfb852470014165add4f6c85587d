package week4
import week4._
object lesson3 {
  println("Welcome to the Scala worksheet")       //> Welcome to the Scala worksheet
  def nth[T](n: Int, xs: List[T]): T = {
		if (n == 0) xs.head
		else nth(n-1, xs.tail)

  }                                               //> nth: [T](n: Int, xs: week4.List[T])T
}