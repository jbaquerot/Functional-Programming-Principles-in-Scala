import common._

object worksheet {
  'a' < 'b'                                       //> res0: Boolean(true) = true
  var l = List(('a',1), ('b',1), ('a',1), ('b',1), ('b',1))
                                                  //> l  : List[(Char, Int)] = List((a,1), (b,1), (a,1), (b,1), (b,1))
  //msort((c,n): (Char, Int), (c1,n1): (Char, Int) => c < c1)(List(('a',1), ('b',1), ('a',1), ('b',1), ('b',1)))
 l sortWith(_._1 > _._1)                          //> res1: List[(Char, Int)] = List((b,1), (b,1), (b,1), (a,1), (a,1))
  //List(1, -3, 4, 2, 6) sortWith (_ < _)
  //(1,5)._1

}